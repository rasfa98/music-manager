{{>flash}}

<div class="row">
    {{#each rows}}
    <div class="card col-sm-6">
        <div class="card-body">
            <h5 class="card-title">{{albumTitle}}</h5>
            <p class="card-text">{{band}}</p>
            <a href="/view?band={{band}}&albumTitle={{albumTitle}}">View album</a>
        </div>
    </div>
    {{/each}}
</div>